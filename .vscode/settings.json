{"vsnotes.noteTitleConvertSpaces": " ",
    "vsnotes.defaultNoteTitle": "{title}.{ext}"/*"{dt} {title}.{ext}"*/,
    "vsnotes.tokens": [
      /*{
        "type": "datetime",
        "token": "{dt}",
        "format": "YYYYMMDD",
        "description": "Insert formatted datetime."
      },*/
      {
        "type": "title",
        "token": "{title}",
        "description": "Insert note title from input box.",
        "format": "Untitled"
      },
      {
        "type": "extension",
        "token": "{ext}",
        "description": "Insert file extension.",
        "format": "md"
      }
    ],

    "vsnotes.templates": [
        "mydefault",
    ],

    "markdown-pdf.margin.top": "1cm",
    "markdown-pdf.margin.bottom": "2cm",
    "markdown-pdf.displayHeaderFooter": true,
    "markdown-pdf.headerTemplate": "<div style=\"font-family: '游ゴシック体', 'Yu Gothic', YuGothic, 'ヒラギノ角ゴ Pro', 'Hiragino Kaku Gothic Pro', 'メイリオ', 'Meiryo', sans-serif; position: relative; border-top: 1px solid black; margin: 0.2cm; font-size: 9px; width: 100%;\"></div>",
    "markdown-pdf.footerTemplate": "<div style=\"font-family: '游ゴシック体', 'Yu Gothic', YuGothic, 'ヒラギノ角ゴ Pro', 'Hiragino Kaku Gothic Pro', 'メイリオ', 'Meiryo', sans-serif; position: relative; border-top: 1px solid black; margin: 0.5cm; font-size: 9px; width: 100%;\"><div style=\"position: absolute; width: 100%; top: 0.2cm; text-align: center;\"><span class='pageNumber'></span> / <span class='totalPages'></span></div><div style=\"position: absolute; right: 0; top: 0.2cm;\">Keigo Enomoto</div></div>",
    "markdown-pdf.type": ["pdf"],
    "markdown-pdf.convertOnSave": true,
    "markdown-pdf.outputDirectory": "~/Scripts/Koishi_simulation/Sim_tutorial/doc/",
    "markdown.preview.breaks": true,
    "markdown-pdf.breaks": true,
    "markdown-pdf.margin.left": "0.5cm",
    "markdown-pdf.margin.right": "0.5cm",
    "pasteImage.path": "~/Scripts/Koishi_simulation/Sim_tutorial/markdown/image/",
}